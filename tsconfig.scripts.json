{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "outDir": "./dist/scripts",
    "rootDir": "./src/scripts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/scripts/**/*"
  ],
  "exclude": [
    "node_modules",
    "src/scripts/testImport.ts"
  ]
}